<style>
    .slider-bg h2 {
        color: #fff;
        font-size: 60px;
        line-height: 70px;
        letter-spacing: 2px;
        font-weight: 700;
        margin-bottom: 20px;
    }

    .overlay-name span {
        display: block;
        width: 100%;
        float: left;
        font-family: 'Aleo';
    }
</style>
<style>
    /*eski haline dönmesi için body-bg içini dışarı kopyala*/
    .body-bg{
        background-image: url(/images/bg.png);
        background-size: 100%;
        background-position: center;
        overflow: hidden;
    }
    .swiper-slide {
        position: relative;
    }

    .swiper-slide .slider-bg {
        position: relative;
        z-index: 1;
        overflow: hidden;
    }

    .swiper-slide .slider-bg::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: rgba(0, 0, 0, 0.5); /* Buradaki 0.5 saydamlık seviyesini dilediğiniz gibi ayarlayabilirsiniz */
        z-index: 2;
    }

    .swiper-slide .slider-bg h2 {
        color: #fff;
        font-size: 60px;
        line-height: 70px;
        letter-spacing: 2px;
        font-weight: 700;
        margin-bottom: 20px;
        position: relative;
        z-index: 3;
    }

    .big-slider .big-slider-swiper .swiper{
        border-radius:0px !important;
    }

    .swiper-slide .slider-bg .btn-big {
        position: relative;
        z-index: 3; /* Butonun z-index değerini artırın */
    }

    /* Diğer stiller buraya eklenmeye devam edebilir */
</style>



<section class="big-slider">
    <div class="big-slider-swiper" style="height:100vh;">
