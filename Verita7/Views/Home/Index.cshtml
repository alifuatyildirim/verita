        // Initialize Slide Items
        function initSlides() {

            // Variables
            var mobileJson = [{ "Id": 1, "Title": "Her zaman <br> mevsiminde taptaze", "Name": "", "PictureUrl": "/i/l/000/0000008.jpeg", "Url": "/", "Mobile": true, "Tablet": true, "Desktop": true }, { "Id": 2, "Title": "Her zaman <br> mevsiminde taptaze 2", "Name": "Slider2", "PictureUrl": "/i/l/000/0000025_slider2.jpeg", "Url": null, "Mobile": true, "Tablet": true, "Desktop": true }];
            var tabletJson = [{ "Id": 1, "Title": "Her zaman <br> mevsiminde taptaze", "Name": "", "PictureUrl": "/i/l/000/0000008.jpeg", "Url": "/", "Mobile": true, "Tablet": true, "Desktop": true }, { "Id": 2, "Title": "Her zaman <br> mevsiminde taptaze 2", "Name": "Slider2", "PictureUrl": "/i/l/000/0000025_slider2.jpeg", "Url": null, "Mobile": true, "Tablet": true, "Desktop": true }];
            var desktopJson = [{ "Id": 1, "Title": "Her zaman <br> mevsiminde taptaze", "Name": "", "PictureUrl": "/i/l/000/0000008.jpeg", "Url": "/", "Mobile": true, "Tablet": true, "Desktop": true }, { "Id": 2, "Title": "Her zaman <br> mevsiminde taptaze 2", "Name": "Slider2", "PictureUrl": "/i/l/000/0000025_slider2.jpeg", "Url": null, "Mobile": true, "Tablet": true, "Desktop": true }];

            // Slide Array
            var slides = new Array();

            // Viewport Detect
            if (ResponsiveToolkit.is('<=sm')) { slides = mobileJson }
            else if (ResponsiveToolkit.is('md')) { slides = tabletJson }
            else { slides = desktopJson }

            // Slide Content Wrapper
            var slideWrap = $('.big-slider-swiper .swiper-wrapper').empty();

            if (slides.length == 0) {
                $('.big-slider').hide();
            }

            // Each slide items in slides
            $.each(slides, function (index, slide) {
                var slideItem = $('<div class="swiper-slide"><div class="slider-bg" style="background-image: url(' + slide.PictureUrl + ')"><h2>' + slide.Title + '</h2><a href="' + slide.Url +'" class="btn-big" style=" position: relative; ">DAHA FAZLA</a><a href="' + slide.Url + '"></a></div>');
                slideWrap.append(slideItem);
            });



            //Swiper-Slider
            const swiper = new Swiper('.big-slider-swiper .swiper', {
                // Enable lazy loading
           
                lazy: {
                    loadPrevNext: true,
                },
                slidesPerView: 1,
                loop: true,
                centeredSlides: true,
                speed: 500,
                effect: 'slide',
                navigation: {
                    nextEl: '.big-slider-swiper .big-slider-button-next',
                    prevEl: '.big-slider-swiper .big-slider-button-prev',
                }
            });
        }

        // DOM on Ready
        $(document).ready(function () {
            initSlides();
        });
    </script>


